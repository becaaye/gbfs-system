{
  "name": "@becaaye/gbfs-system",
  "version": "1.0.0",
  "description": "A Node.js package that enables real-time data retrieval from GBFS (General Bikeshare Feed Specification) using the auto-discovery URLs provided by MobilityData. It allows developers to easily access and utilize live bikeshare system information, station information, station status, etc, and other valuable data for building innovative applications and services.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./Systems": {
      "import": "./dist/Systems.js",
      "require": "./dist/Systems.js",
      "types": "./dist/Systems.d.ts"
    },
    "./Gbfs": {
      "import": "./dist/Gbfs.js",
      "require": "./dist/Gbfs.js",
      "types": "./dist/Gbfs.d.ts"
    }
  },

  "scripts": {
    "clean": "rimraf dist/",
    "include" : "cp package.json src/types.d.ts dist/",
    "build": "npm run clean && tsc && npm run include",
    "test": "jest",
    "prepublishOnly": "npm test && npm run build",

    "debug": "npm run build && node --inspect-brk=9229 ./dist/esm/index.js",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becaaye/gbfs-system.git"
  },
  "keywords": [
    "gbfs",
    "feeds",
    "systems",
    "bikeshare",
    "realtime",
    "stations"
  ],
  "author": "Becaye Badiane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/becaaye/gbfs-system/issues"
  },
  "homepage": "https://github.com/becaaye/gbfs-system#readme",
  "dependencies": {
    "axios": "^1.5.1",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
